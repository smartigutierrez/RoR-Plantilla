<!DOCTYPE html>
<html>
  <head>
    <title>Galeria Interactiva</title>
	<meta charset='utf-8' />
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>
	<meta name='viewport' content='width=device-width, initial-scale=1, user-scalable=no' />
	<%= stylesheet_link_tag 'https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.15.4/css/all.min.css' %>
	<%= stylesheet_link_tag 'main.css' %>
	<noscript> <%= stylesheet_link_tag 'noscript.css' %> </noscript>    
  </head>

<%= yield%>

<!-- Scripts -->
	<%= javascript_include_tag 'jquery.min.js' %>
	<%= javascript_include_tag 'browser.min.js' %>
	<%= javascript_include_tag 'breakpoints.min.js' %>
	<%= javascript_include_tag 'main.js' %>
	<%= javascript_include_tag 'application.js' %>
	<%= javascript_importmap_tags %>


</html>
